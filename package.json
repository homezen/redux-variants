{
  "name": "redux-variants",
  "version": "1.0.2",
  "description": "Universal JavaScript variants system for Redux",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "npm run clean && npm run build-js",
    "build-js": "BLUEBIRD_DEBUG=1 NODE_ENV=production babel src -s -d lib",
    "check-deps": "npm outdated && npm-check || echo \"deps out of date!\"",
    "clean": "rm -vrf ./lib",
    "codecov": "cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js",
    "deploy": "npm run build && npm run release",
    "dev": "npm run watch",
    "lint": "npm run lint-eslint && npm run lint-remark && npm run lint-yaml",
    "lint-eslint": "eslint --ignore-path .gitignore .",
    "lint-yaml": "bundle exec yaml-lint *.yml *.yaml",
    "lint-remark": "remark -f src/**/*.md *.md",
    "lint-watch": "watch 'eslint --ignore-path .gitignore .' src",
    "release": "NODE_ENV=production BLUEBIRD_DEBUG=1 ./do-release.sh",
    "start": "npm run dev",
    "test": "npm run test-mocha && npm run test-karma",
    "test-karma": "karma start",
    "test-mocha": "babel-node ./node_modules/.bin/isparta cover _mocha -- -R ${MOCHA_REPORTER-'nyan'} ./src/**/*.test.js ./src/*.test.js",
    "test-mocha-junit": "mocha --compilers js:babel-core/register -R mocha-junit-reporter --reporter-options mochaFile=$CIRCLE_TEST_REPORTS/mocha/docs-api.xml  ./src/**/*.test.js",
    "test-watch": "mocha -w --compilers js:babel-core/register -R dot ./src/**/*.test.js ./src/*.test.js",
    "watch": "npm run watch-js & NODE_ENV=production npm run test-watch & npm run lint-watch",
    "watch-js": "npm run clean && babel src -w -s -d lib"
  },
  "author": "SpainTrain",
  "license": "MIT",
  "engines": {
    "node": "^4 || ^6",
    "npm": "^2 || ^3"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.2.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-node": "^6.1.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.3.0",
    "chai-enzyme": "^0.5.0",
    "codecov.io": "^0.1.6",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-plugin-lodash": "^1.10.1",
    "eslint-plugin-lodash-fp": "^2.0.0",
    "eslint-plugin-react": "^5.2.2",
    "isparta": "^4.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "mocha-junit-reporter": "^1.11.1",
    "nock": "^8.0.0",
    "nodemon": "^1.9.2",
    "npm-check": "^5.2.2",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "redux": "^3.5.2",
    "remark": "^5.0.1",
    "remark-cli": "^1.0.0",
    "remark-lint": "^4.0.1",
    "sinon": "^2.0.0-pre",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4",
    "watch": "^0.19.1",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "debug": "^2.2.0",
    "es6-error": "^3.0.0",
    "lodash": "^4.13",
    "react-redux": "^4.4.5",
    "reselect": "^2.5.3"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/homezen/redux-variants.git"
  },
  "bugs": {
    "url": "https://github.com/homezen/redux-variants/issues"
  },
  "homepage": "https://github.com/homezen/redux-variants#readme"
}
